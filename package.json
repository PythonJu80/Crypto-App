{
  "name": "crypto-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node -r dotenv/config src/server.js",
    "dev": "vite",
    "build": "vite build",
    "build:prod": "NODE_ENV=production npm run validate:env && npm run clean && vite build && node scripts/validate-build.js",
    "build:dev": "NODE_ENV=development npm run validate:env && npm run clean && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "clean": "rimraf dist",
    "db:init": "node src/database/init.js",
    "db:seed": "node src/database/seed.js",
    "db:verify": "node src/database/verifySeed.js",
    "validate:env": "node scripts/load-secrets.js",
    "validate:build": "node scripts/validate-build.js",
    "deploy:prod": "npm run test && npm run build:prod && npm run start:prod",
    "start:prod": "NODE_ENV=production node -r dotenv/config src/server.js",
    "validate": "node scripts/validate-build.js"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.39.1",
    "@opentelemetry/sdk-metrics": "^1.12.0",
    "@sentry/node": "^7.80.1",
    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.3",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.12.2",
    "socket.io-client": "^4.7.4",
    "sqlite3": "^5.1.7",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitejs/plugin-vue": "^4.5.0",
    "autoprefixer": "^10.4.18",
    "babel-jest": "^29.7.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.2.3",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.4.1",
    "terser": "^5.36.0",
    "vite": "^5.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.21.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
